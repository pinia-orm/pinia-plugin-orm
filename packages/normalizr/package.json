{
  "name": "@pinia-plugin-orm/normalizr",
  "type": "module",
  "version": "0.0.0",
  "description": "Normalizes JSON according to schema for Redux and Flux applications",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "pinia-plugin",
    "orm",
    "normalizr"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup"
  }
}
